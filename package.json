{
  "name": "beewar-fe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "http-proxy-middleware": "^1.0.6",
    "jwt-decode": "^3.1.1",
    "pixi.js": "^5.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "prefer-const": "warn",
      "no-var": "warn",
      "func-style": [
        "warn",
        "declaration",
        {
          "allowArrowFunctions": true
        }
      ],
      "no-param-reassign": "warn",
      "prefer-template": "error",
      "template-curly-spacing": "error",
      "arrow-spacing": "error",
      "arrow-body-style": "error",
      "no-trailing-spaces": [
        "warn",
        {
          "skipBlankLines": true
        }
      ],
      "no-irregular-whitespace": "error",
      "no-multi-spaces": "warn",
      "space-before-blocks": "warn",
      "space-before-function-paren": [
        "warn",
        "never"
      ],
      "space-in-parens": [
        "error",
        "never"
      ],
      "eol-last": [
        "error",
        "always"
      ],
      "semi": [
        "error",
        "never"
      ],
      "curly": "error",
      "dot-notation": "error",
      "quotes": [
        "error",
        "single"
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "comma-dangle": [
        "warn",
        "always-multiline"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://beewar.herokuapp.com"
}
